def drop_non_stat(df):
  adf = df.drop(["Year"], axis = 1)
  for name, column in adf.iteritems():
    if not adfuller_simple(column, name=column.name):
      adf = adf.drop([column], axis = 1)
  return adf


def drop_non_stat(df):
  adf = df.drop(["Year"], axis = 1)
  for name, column in adf.iteritems():
      if not adfuller_simple(column, name=column.name):
        adf = adf.drop([column.name], axis = 1)
      else:
        continue
  return adf


